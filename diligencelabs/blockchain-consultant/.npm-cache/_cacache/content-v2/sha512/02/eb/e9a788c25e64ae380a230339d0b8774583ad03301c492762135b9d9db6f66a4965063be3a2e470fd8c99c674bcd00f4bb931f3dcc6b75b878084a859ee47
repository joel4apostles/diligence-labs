ession and syncs it with WAGMI.\n","exports":{".":{"types":"./dist/index.d.ts","import":"./dist/esm/index.js"}},"gitHead":"5495e6f24cc660ae5406643cb8b1da2543026f51","scripts":{"lint":"prettier --check \"src/**/*.{ts,tsx,js,jsx}\" && npx tsc --noEmit","build":"npx tsup --clean --minify","clean":"rm -rf ./dist","watch":"npx tsup --watch","format":"prettier --write \"src/**/*.{ts,tsx,js,jsx}\"","prepublishOnly":"npm run clean && npm run build"},"typings":"./dist/index","_npmUser":{"name":"ahollenbach","email":"hollenbach.andrew@gmail.com"},"repository":{"url":"git+https://github.com/privy-io/wagmi-connector.git","type":"git"},"_npmVersion":"9.8.1","description":"A connector for Privy <> WAGMI.sh","directories":{},"_nodeVersion":"18.19.0","_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"tsup":"^7.1.0","prettier":"^2.8.3","typescript":"^5.1.6","@types/node":"^18.11.18","@types/react":"^18.0.26","@tsconfig/node18":"^18.2.0","semantic-release":"^21.0.7","@tsconfig/strictest":"^2.0.1","@semantic-release/git":"^10.0.1","@saithodev/semantic-release-backmerge":"^2.2.0"},"peerDependencies":{"viem":">=0.3.35","react":"^18","wagmi":">=1.4.12 <2","react-dom":"^18","@privy-io/react-auth":"^1.33.0"},"_npmOperationalInternal":{"tmp":"tmp/wagmi-connector_0.1.9-beta.1_1702651351652_0.10778572553308963","host":"s3://npm-registry-packages"}},"0.1.9":{"name":"@privy-io/wagmi-connector","version":"0.1.9","keywords":["authentication","privy","wagmi","connector","web3"],"author":{"name":"privy.io"},"license":"Apache-2.0","_id":"@privy-io/wagmi-connector@0.1.9","maintainers":[{"name":"koleok","email":"kylechamberlain@proton.me"},{"name":"joshnaviprivy","email":"josh@privy.io"},{"name":"payton-privy","email":"payton@privy.io"},{"name":"privy-bot","email":"bot@privy.io"},{"name":"ankushswar1","email":"ankush@privy.io"},{"name":"ahollenbach","email":"hollenbach.andrew@gmail.com"},{"name":"asta-li","email":"asta@privy.io"},{"name":"sternhenri","email":"henri@privy.io"}],"ho