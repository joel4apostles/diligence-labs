ols the session, and syncs it with WAGMI.\n\n\n## Alpha\n\nThis project is in alpha, please share issues with the team directly or in the [Github repository](https://github.com/privy-io/wagmi-connector/issues).\n","exports":{".":{"types":"./dist/index.d.ts","import":"./dist/esm/index.js"}},"gitHead":"67ee13981dfff5609a78fafd0bcc167b1bcdef5a","scripts":{"lint":"prettier --check \"src/**/*.{ts,tsx,js,jsx}\" && npx tsc --noEmit","build":"npx tsup --clean --minify","clean":"rm -rf ./dist","watch":"npx tsup --watch","prepublishOnly":"npm run clean && npm run build"},"typings":"./dist/index","_npmUser":{"name":"ahollenbach","email":"hollenbach.andrew@gmail.com"},"repository":{"url":"git+https://github.com/privy-io/wagmi-connector.git","type":"git"},"_npmVersion":"8.11.0","description":"A connector for Privy <> WAGMI.sh","directories":{},"_nodeVersion":"16.15.1","_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"tsup":"^6.5.0","prettier":"^2.8.3","typescript":"^4.9.4","@types/node":"^18.11.18","@types/react":"^18.0.26","@tsconfig/node16-strictest-esm":"^1.0.3"},"peerDependencies":{"react":"^18.2","wagmi":">=0.10.0","ethers":"^5.7.2","react-dom":"^18.2.0","@privy-io/react-auth":">=1.26.0-beta.1"},"_npmOperationalInternal":{"tmp":"tmp/wagmi-connector_0.0.9-beta.2_1685401685243_0.3545048916857827","host":"s3://npm-registry-packages"}},"0.0.9":{"name":"@privy-io/wagmi-connector","version":"0.0.9","keywords":["authentication","privy","wagmi","connector","web3"],"author":{"name":"privy.io"},"license":"Apache-2.0","_id":"@privy-io/wagmi-connector@0.0.9","maintainers":[{"name":"payton-privy","email":"payton@privy.io"},{"name":"privy-bot","email":"bot@privy.io"},{"name":"ankushswar1","email":"ankush@privy.io"},{"name":"nichochar","email":"nicholascharriere@gmail.com"},{"name":"ahollenbach","email":"hollenbach.andrew@gmail.com"},{"name":"benjreinhart","email":"benjreinhart@gmail.com"},{"name":"asta-li","email":"asta@privy.io"},{"name":"sternhenri","email":"henri